<?xml version="1.0" encoding="utf-8"?>














<com.android.camera.ui.ListPrefSettingPopup style="@style/SettingPopupWindow"
  xmlns:android="http://schemas.android.com/apk/res/android"
>
    <LinearLayout android:orientation="vertical" android:background="@color/popup_background" android:layout_height="wrap_content" android:layout_width="@dimen/setting_popup_window_width"



>
        <FrameLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:minHeight="@dimen/popup_title_frame_min_height"


>
            <TextView android:id="@id/title" style="@style/PopupTitleText"
 />
        </FrameLayout>

        <View style="@style/PopupTitleSeparator" />

        <FrameLayout android:layout_height="wrap_content" android:layout_width="fill_parent"
>
            <ListView android:id="@id/settingList" android:choiceMode="singleChoice" style="@style/SettingItemList"

 />
        </FrameLayout>
    </LinearLayout>
</com.android.camera.ui.ListPrefSettingPopup>
